.multiselect-dropdown {
    position: absolute;
    left: -1px;
    right: -1px;
    bottom: 0;
    transform: translateY(100%);
    /*border: 1px solid $color-green;*/
    margin-top: -1px;
    max-height: 20rem;
    overflow-y: scroll;
    -webkit-overflow-scrolling: touch;
    z-index: 100;
    background: $color-white;
    border: 1px solid $color-gray-T5;
    display: flex;
    flex-direction: column;
    border-radius: 0 0 6px 6px;
    outline: none;
    @mixin scrollbar $color-shadow, $color-green, 10px;

    &.is-top {
        transform: translateY(-100%);
        top: 1px;
        bottom: auto;
        flex-direction: column-reverse;
        border-radius: 6px 6px 0 0;
    }

    &.is-hidden {
        display: none;
    }
}

.multiselect-group {
    padding: 12px 12px 0;
}

.multiselect-group-label {
    font-weight: 500;
    font-size: 14px;
    color: $color-gray-T1;
    padding-bottom: 6px;
    
}

.multiselect-group-options {
    & .multiselect-option {
        padding: 4px 12px;
        font-weight: 300;
    }
}

.multiselect-options {
    padding: 0;
    margin: 0;
    list-style: none;
    display: flex;
    flex-direction: column;
    max-height: var(--ms-max-height, 10rem);

    &.is-top {
        flex-direction: column-reverse;
    }

    & .multiselect-option.is-disabled {
        
    }
}

.multiselect-no-options,
.multiselect-no-results {
    padding: var(--ms-option-py, 0.5rem) var(--ms-option-px, 0.75rem);
    color: var(--ms-empty-color, #4B5563);
}

.multiselect-fake-input {
    background: transparent;
    position: absolute;
    left: 0;
    right: 0;
    bottom: -1px;
    width: 100%;
    height: 1px;
    border: 0;
    padding: 0;
    font-size: 0;
    outline: none;

    &:active, &:focus {
        outline: none;
    }
}

.multiselect-spacer {
    display: none;
}


.multiselect-option {
    &:not(:last-child) {
        & .multiselect-inn {
            border-bottom: 1.5px solid $color-gray-T5;
        }    
    }
}

.multiselect-inn {
    width: 100%;
    margin-right: 10px;    
    
    &__inner {
        display: flex;
        gap: 10px;
        width: 100%;
        font-weight: 300;
        font-size: 12px;
        margin-bottom: 10px;
    }

    &__left {
        flex-basis: 20%;
    }
    
    &__right {
        flex-basis: 80%;
        padding-left: 10px;
        border-left: 1.5px solid $color-gray-T5;
    }

    &__name-city {
        display: flex;
        justify-content: space-between;
        gap: 10px;
    }

    & span {
        font-weight: 400;
    }
}

@keyframes multiselect-spin {
    from {
        transform: rotate(0);
    }
    to {
        transform: rotate(360deg);
    }
}