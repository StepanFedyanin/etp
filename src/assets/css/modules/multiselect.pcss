.multiselect {
    --ms-tag-bg: $color-green;
    --ms-tag-mx: 0;
    --ms-py: 0;
    --ms-px: 12px;
}

.multiselect {
    @mixin select;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    cursor: pointer;
    outline: none;
    min-height: 48px;
    background-color: $color-white;
    padding-top: 7px;
    padding-bottom: 7px;
    padding-right: 60px;

    /*z-index: 10;*/

    &.is-open {
        border-radius: 8px 8px 0 0;
    }

    &.is-open-top {
        border-radius: 0 0 8px 8px;
    }

    &.is-disabled {
        cursor: default;
        color: $color-gray;
    }

    &.is-active {
        /*box-shadow: 0 0 0 var(--ms-ring-width, 3px) var(--ms-ring-color, #10B98130);*/
    }

    &-multiple-label,
    &-single-label,
    &-placeholder {
        display: flex;
        align-items: center;
        height: 100%;
        position: absolute;
        left: 0;
        top: 0;
        pointer-events: none;
        background: transparent;
        line-height: 24px;
        padding-left: 12px;
    }


    &-single-label {
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        max-width: calc(100% - 64px);
    }

    &-placeholder {
        color: $color-gray-T4;
        opacity: 0.5;
    }

    &-caret {
        transform: rotate(0deg);
        transition: .3s transform;
        background-image: url('./assets/img/icons/icon-arrow-select.svg');
        background-position: 50% 50%;
        background-repeat: no-repeat;
        /*background-size: 14px 8px;*/
        width: 14px;
        height: 8px;
        /*position: absolute;*/
        margin-right: -44px;
        z-index: 10;
        pointer-events: none;

        &.is-open {
            transform: rotate(180deg);
            pointer-events: auto;
        }
    }

    &-option {
        display: flex;
        box-sizing: border-box;
        text-decoration: none;
        align-items: center;
        justify-content: flex-start;
        text-align: left;
        cursor: pointer;
        font-size: 14px;
        line-height: 22px;
        padding: 10px 30px 10px 15px;

        &.is-pointed {
            background: $color-white-dark;
            color: $color-black;
        }

        &.is-disabled {
            color: $color-gray;
            cursor: not-allowed;
        }

        &.is-selected {
            background-image: url('./assets/img/icons/icon-check.svg');
            background-position: calc(100% - 14px) 50%;
            background-repeat: no-repeat;
            /*background-size: 16px 12px;*/
            background-color: $color-green;
            color: $color-white;
        }

        &.is-selected.is-pointed {
            background-color: $color-green-dark;
        }

        &.is-selected.is-disabled {
            background-color: $color-gray;
        }
    }

    &.m--checkbox &-option {
        position: relative;
        padding-right: 15px;
        padding-left: 45px;

        &:before {
            content: '';
            position: absolute;
            top: 50%;
            margin-top: -12px;
            left: 12px;
            background: none;
            display: block;
            width: 24px;
            height: 24px;
            border: 1.5px solid $color-gray-T5;
            border-radius: 6px;

        }

        &.is-selected {
            background: $color-white-dark;
            color: $color-black;
            &:before {
                background-color: $color-green;
                border: 1.5px solid $color-green;
                background-image: svg-load('assets/img/icons/icon-check.svg', fill=$color-white);
                background-repeat: no-repeat;
                background-position: center;
                background-size: 18px;
            }
        }
    }

}

.multiselect-search {
    width: 100%;
    position: absolute;
    top: 0px;
    bottom: 0px;
    left: 0px;
    right: 0px;
    outline: none;
    box-sizing: border-box;
    border: 0;
    appearance: none;
    font-size: inherit;
    font-family: inherit;
    background: var(--ms-bg, transparent);
    border-radius: var(--ms-radius, 4px);
    padding-left: var(--ms-px, 0.875rem);
}

.multiselect-tags {
    flex-grow: 1;
    flex-shrink: 1;
    display: flex;
    flex-wrap: wrap;
    margin: var(--ms-tag-my, 0.25rem) auto 0;
    padding-left: var(--ms-py, 0.5rem);
    align-items: center;
    max-width: 100%;
}

.multiselect-tag {
    background: var(--ms-tag-bg, #10B981);
    color: var(--ms-tag-color, #FFFFFF);
    font-size: var(--ms-tag-font-size, 0.875rem);
    line-height: var(--ms-tag-line-height, 1.25rem);
    font-weight: var(--ms-tag-font-weight, 600);
    padding: var(--ms-tag-py, 0.125rem) 0 var(--ms-tag-py, 0.125rem) var(--ms-tag-px, 0.5rem);
    border-radius: var(--ms-tag-radius, 4px);
    margin-right: var(--ms-tag-mr, 0.25rem);
    margin-bottom: var(--ms-tag-my, 0.25rem);
    display: flex;
    align-items: center;
    /*white-space: nowrap;*/
    max-width: 100%;

    &.is-disabled {
        padding-right: var(--ms-tag-px, 0.5rem);
        background: var(--ms-tag-bg-disabled, #9CA3AF);
        color: var(--ms-tag-color-disabled, #FFFFFF);
    }
}

.multiselect-tag-remove {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: var(--ms-tag-remove-py, 0.25rem) var(--ms-tag-remove-px, 0.25rem);
    margin: var(--ms-tag-remove-my, 0rem) var(--ms-tag-remove-mx, 0.125rem);
    border-radius: var(--ms-tag-remove-radius, 4px);
    
    &:hover {
        background: #00000010;
    }
}

